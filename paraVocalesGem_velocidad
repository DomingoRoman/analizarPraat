Extract non-empty intervals: 2, "no"

Scale peak: 0.99

nSounds = numberOfSelected("Sound")

for i to nSounds
	frase [i] = selected ("Sound", i)
endfor

To TextGrid (silences): 100, 0, -25, 0.1, 0.1, "", "1"

nTGrids = numberOfSelected("TextGrid")

for i to nTGrids
	tg [i] = selected ("TextGrid", i)
	tg$ [i] = selected$ ("TextGrid", i)
endfor

for i to nTGrids
	selectObject: tg [i]
	tg$ = selected$ ("TextGrid")
	nintervalos = Get number of intervals: 1
		if nintervalos = 5
			writeInfoLine: "Ojo, m√°s de dos"
			Remove left boundary: 1, 4
			Remove left boundary: 1, 3
		elsif nintervalos = 4
			Remove left boundary: 1, 3
		endif
	Set interval text: 1, 2,tg$
View & Edit alone

endfor

for i to nTGrids
	selectObject: tg [i]
	plus frase [i]
	View & Edit
	pause
endfor

tabla = Create Table with column names: "tablaDurEnunc", nTGrids, "informante frase duracion"

for i to nTGrids
	selectObject: tg [i]
	ninter = Get number of intervals: 1
	fin = Get end time of interval: 1, 2
	ini = Get start time of interval: 1, 2
	dur = fin-ini
	ndefrase = Get label of interval: 1, 2
	if ndefrase = 4
		nfonemas = 14
	elsif ndefrase = 5
		nfonemas = 12
	elsif ndefrase = 6
		nfonemas = 18
	elsif ndefrase = 9
		nfonemas = 12
	elsif ndefrase = 14
		nfonemas = 14
	elsif ndefrase = 15
		nfonemas = 15
	elsif ndefrase = 16
		nfonemas = 12
	elsif ndefrase = 17
		nfonemas = 14
	elsif ndefrase = 22
		nfonemas = 14
	elsif ndefrase = 23
		nfonemas = 15
	elsif ndefrase = 24
		nfonemas = 19
	elsif ndefrase = 25
		nfonemas = 15
elsif ndefrase = 26
		nfonemas = 15
elsif ndefrase = 27
		nfonemas = 10
elsif ndefrase = 28
		nfonemas = 18
elsif ndefrase = 29
		nfonemas = 10
elsif ndefrase = 30
		nfonemas = 15
elsif ndefrase = 32
elsif ndefrase = 34
elsif ndefrase = 38
elsif ndefrase = 39
elsif ndefrase = 41
elsif ndefrase = 42
elsif ndefrase = 44
elsif ndefrase = 47
elsif ndefrase = 48
elsif ndefrase = 49
elsif ndefrase = 50

	endif

	select tabla



endfor		
